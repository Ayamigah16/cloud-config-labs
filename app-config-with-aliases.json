    "maintainers": [
      {
        "name": "Alice Smith",
        "email": "alice@example.com"
      },
      {
        "name": "Bob Lee",
        "email": "bob@example.com"
      }
    ]
  },
  "server": {
    "host": "0.0.0.0",
    "port": 8080,
    "protocol": "https",
