{
  "application": {
    "name": "my-web-app",
    "version": "1.0.0",
    "environment": "production",
    "description": "Minimal web application config",
    "maintainers": [
      {
        "name": "Alice Smith",
        "email": "alice.smith@example.com"
      },
      {
        "name": "Bob Lee",
        "email": "bob.lee@example.com"
      }
    ]
  },
  "server": {
    "host": "${server_host}",
    "port": "${server_port}",
    "protocol": "https",
    "cors": {
      "enabled": true,
      "allowed_origins": "${allowed_origins}"
    }
  },
  "database": {
    "engine": "mysql",
    "host": "${db_host}",
    "port": "${db_port}",
    "username": "${db_username}",
    "password": "${db_password}",
    "dbname": "${db_name}",
    "pool_size": 10
  },
  "logging": {
    "level": "info",
    "format": "json",
    "file": "/var/log/mywebapp/app.log"
  },
  "app_ref": {
    "name": "my-web-app",
    "version": "1.0.0",
    "environment": "production",
    "description": "Minimal web application config",
    "maintainers": [
      {
        "name": "Alice Smith",
        "email": "alice.smith@example.com"
      },
      {
        "name": "Bob Lee",
        "email": "bob.lee@example.com"
      }
    ]
  },
  "server_ref": {
    "host": "${server_host}",
    "port": "${server_port}",
    "protocol": "https",
    "cors": {
      "enabled": true,
      "allowed_origins": "${allowed_origins}"
    }
  },
  "database_ref": {
    "engine": "mysql",
    "host": "${db_host}",
    "port": "${db_port}",
    "username": "${db_username}",
    "password": "${db_password}",
    "dbname": "${db_name}",
    "pool_size": 10
  },
  "logging_ref": {
    "level": "info",
    "format": "json",
    "file": "/var/log/mywebapp/app.log"
  }
}